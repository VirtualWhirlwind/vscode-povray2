{
    "Camera": {
        "prefix": "camera",
        "description": "Insert a Camera",
        "scope": "povray",
        "body": [
            "camera {",
            "\tlocation <${1:x}, ${2:y}, ${3:z}>",
            "\tright     x*image_width/image_height // keep propotions regardless of aspect ratio",
            "\tlook_at  <${4:x}, ${5:y},  ${6:z}>",
            "}${0}"
        ]
    },


    "Point Light": {
        "prefix": "light_source",
        "description": "Insert a Point Light",
        "scope": "povray",
        "body": [
            "light_source {",
            "\t<${1:x}, ${2:y}, ${3:z}>",
            "\tcolor <${4:1.0}, ${5:1.0},  ${6:1.0}> // <red, green, blue>",
            "\t${0}",
            "}"
        ]
    },
    "Spotlight": {
        "prefix": "light_source",
        "description": "Insert a Spotlight",
        "scope": "povray",
        "body":[
            "light_source {",
            "\t<${1:x}, ${2:y}, ${3:z}>",
            "\tcolor <${5:1.0}, ${6:1.0},  ${7:1.0}> // <red, green, blue>",
            "\tspotlight radius ${8:5} falloff ${9:10} tightness ${10:0}",
            "\tpoint_at <${11:0}, ${12:0}, ${13:0}>",
            "\t${0}",
            "}"
        ]
    },
    "Cylinder Light": {
